{
  "name": "resume",
  "version": "0.1.0",
  "description": "A simple data-driven resume creator",
  "main": "index.tsx",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start:web": "npm run build:static && webpack-dev-server --config-name resume-web",
    "build:web": "npm run build:static && webpack --config-name resume-web-prod",
    "build:web:confidential": "npm run build:static:confidential && webpack --config-name resume-web-prod",
    "build:static": "webpack --config-name resume-ssr && npm run ssr:public",
    "build:static:confidential": "webpack --config-name resume-ssr && ssr:confidential",
    "ssr:confidential": "CONFIDENTIAL_MODE=true node ./dist/main.bundle.js",
    "ssr:public": "node ./dist/main.bundle.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/webpack-bundle-analyzer": "^2.13.0",
    "@types/webpack-merge": "^4.1.3",
    "@types/webpack-node-externals": "^1.6.3",
    "autoprefixer": "^9.4.6",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "preact": "^8.4.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
